      "source": [
        "# 消費税を加えた金額を計算する関数\n",
        "def calculate_tax(price, tax_rate):\n",
        "    tax = price * tax_rate / 100\n",
        "    total = price + tax\n",
        "    return total\n",
        "\n",
        "# 商品金額110円、消費税10%で計算\n",
        "result = calculate_tax(110, 10)\n",
        "\n",
        "# 結果を表示\n",
        "print(result)\n"
      ]
    }
  ]
